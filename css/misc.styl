@font-face {
    font-family: Wisdom;
    src: url(../img/wisd.otf);
}
@font-face {
    font-family: Days;
    src: url(../img/days.otf);
}



.intro-text {
    width: 100%;
    margin: 60px auto 15px;
    cursor: default;
    -webkit-transform: skewY(-8deg);
    -moz-transform: skewY(-8deg);
    -o-transform: skewY(-8deg);
    -ms-transform: skewY(-8deg);
    transform: skewY(-8deg);
    -webkit-backface-visibility: hidden;
    -moz-backface-visibility: hidden;
    text-align: center;
}

.intro-text span {
    display: block;
    color: #fff;
    font-weight: 400;
    text-transform: uppercase;
    margin-top: 3px;
    font-family: Days;
}

.intro-text span:first-child {
    display: inline;
    font-size: 50px;
    word-spacing: 25px;
    letter-spacing: 12px;
    text-shadow: 2px 2px 0px $color2;
}

.intro-text span:nth-child(2) {
    font-family: Wisdom, cursive;
    font-weight: 400;
    text-transform: none;
    font-style: italic;
    line-height: 30px;
    font-size: 74px;
    color: $color2
    letter-spacing: 5px
    text-shadow: 1px 2px 0px rgba(255,255,255,1);
    position: relative;
}

.intro-text span:nth-child(2):before,
.intro-text span:nth-child(2):after {
    content: "";
    width: 35%;
    height: 2px;
    background: $color2;
    position: absolute;
    top: 50%;
    margin-top: -2px;
    box-shadow: 1px 1px 1px rgba(255,255,255,1);
}

.intro-text span:nth-child(2):before {
    left: 0px;
}

.intro-text span:nth-child(2):after {
    right: 0px;
}

.intro-text span:nth-child(3) {
    margin-top: 20px;
    font-size: 120px;
    text-shadow: 
        2px 2px 0 $color2,
        4px 4px 0 #fff,
        6px 6px 0 $color2,
        8px 8px 0 #fff;
}











